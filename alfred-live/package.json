{
  "name": "@git-stunts/alfred-live",
  "version": "0.10.3",
  "description": "In-memory control plane for Alfred: adaptive values, config registry, command router.",
  "type": "module",
  "sideEffects": false,
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    }
  },
  "bin": {
    "alfredctl": "./bin/alfredctl.js"
  },
  "dependencies": {
    "@git-stunts/alfred": "0.10.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "author": "James Ross <james@flyingrobots.dev>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/git-stunts/alfred.git"
  },
  "files": [
    "bin",
    "src",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "jsr.json",
    "package.json"
  ],
  "keywords": [
    "resilience",
    "control-plane",
    "runtime-config",
    "live-config",
    "feature-flags",
    "ops"
  ],
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "vitest": "^2.1.8"
  }
}
