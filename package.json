{
  "name": "@git-stunts/alfred-workspace",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "test": "pnpm -r test",
    "lint": "pnpm -r run lint",
    "format": "pnpm -r run format",
    "demo:web": "pnpm --filter @git-stunts/alfred demo:web",
    "demo:web:install": "pnpm --filter @git-stunts/alfred demo:web:install",
    "test:browser": "pnpm --filter @git-stunts/alfred test:browser",
    "release:aggregate": "node scripts/release/aggregate.mjs --write",
    "release:preflight": "node scripts/release/preflight.mjs",
    "release:patch": "node scripts/release/bump.mjs patch",
    "release:minor": "node scripts/release/bump.mjs minor",
    "release:major": "node scripts/release/bump.mjs major",
    "release:publish": "node scripts/release/publish.mjs"
  }
}
